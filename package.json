{
    "name": "first-scandi",
    "version": "0.0.1",
    "license": "OSL-3.0",
    "dependencies": {
        "@scandipwa/eslint-config": "0.1.32",
        "@scandipwa/scandipwa": "4.5.1",
        "@scandipwa/scandipwa-scripts": "2.4.17",
        "@tilework/mosaic": "0.1.4",
        "@tilework/mosaic-craco": "0.0.5",
        "classnames": "^2.3.1"
    },
    "scandipwa": {
        "type": "theme",
